{
  "name": "@e-n-v/env",
  "version": "0.0.4",
  "license": "MIT",
  "homepage": "https://github.com/agorischek/e-n-v",
  "repository": "https://agorischek@github.com/agorischek/e-n-v.git",
  "type": "module",
  "main": "./dist/bundle.cjs",
  "module": "./dist/bundle.js",
  "types": "./dist/bundle.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "e-n-v": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/bundle.d.ts",
      "import": "./dist/bundle.js",
      "require": "./dist/bundle.cjs"
    },
    "./vars": {
      "types": "./dist/vars.d.ts",
      "import": "./dist/vars.js",
      "require": "./dist/vars.cjs"
    }
  },
  "dependencies": {
    "@clack/core": "1.0.0-alpha.6",
    "commander": "^11.1.0",
    "picocolors": "^1.1.1",
    "tsx": "^4.15.7",
    "zod": "^4.0.0"
  },
  "scripts": {
    "bundle": "tsdown"
  }
}
