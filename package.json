{
  "name": "ask-env",
  "version": "1.0.0",
  "description": "Interactive CLI tool to generate .env files with Zod schema validation",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node",
    "test": "bun test",
    "dev": "bun run src/index.ts",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "env",
    "environment",
    "zod",
    "cli",
    "interactive",
    "prompts"
  ],
  "author": "Alex Gorischek",
  "license": "MIT",
  "dependencies": {
    "@clack/core": "1.0.0-alpha.5",
    "@clack/prompts": "0.11.0",
    "picocolors": "^1.1.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
